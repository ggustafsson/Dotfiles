snippet for
	for $1 {
		$2
	}
snippet func
	func $1($2) $3 {
		$4
	}
snippet func-anon
	func($1) {
		$2
	}($3)
snippet if
	if $1 {
		$2
	}
snippet if-err
	if err != nil {
		$1
	}
snippet struct
	type $1 struct {
		$2
	}
snippet switch
	switch $1 {
	case "$2":
		$3
	case "$4":
		$5
	default:
		$6
	}
snippet type
	type $1 {
		$2
	}
